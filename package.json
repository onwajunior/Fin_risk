{
  "name": "financial-risk-analyzer",
  "version": "1.0.0",
  "description": "AI-powered financial risk analysis platform for C-suite executives",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "nodemon server.js",
    "dev:full": "concurrently \"npm run dev\" \"cd frontend && npm start\"",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install",
    "postinstall": "npm run build:frontend",
    "health": "curl -f http://localhost:3001/api/health || exit 1",
    "deploy:vercel": "vercel --prod",
    "clean": "rm -rf node_modules frontend/node_modules frontend/build",
    "reset": "npm run clean && npm run install:all"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "openai": "^4.20.1",
    "axios": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "csv-parser": "^3.0.0",
    "puppeteer": "^21.5.2",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "keywords": ["financial", "risk", "analysis", "ai", "altman", "z-score"],
  "author": "Financial Risk Analyzer Team",
  "license": "MIT"
}